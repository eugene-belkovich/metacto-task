# Server
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Database
MONGODB_URI=mongodb://localhost:27017/feature-voting

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# Cache
CACHE_ENABLED=false
CACHE_TTL_SECONDS=60

# CORS
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info
